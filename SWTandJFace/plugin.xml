<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            label="Sample Action Set"
            visible="true"
            id="swtJFaceDojo.actionSet">
         <menu
               label="SWT Dojo &amp;Menu"
               id="sampleMenu">
            <separator
                  name="sampleGroup">
            </separator>
         </menu>
         <action
               label="Information Action"
               icon="icons/sample.gif"
               class="swtjfacedojo.actions.InfoAction"
               tooltip="Hello, Eclipse world"
               menubarPath="sampleMenu/sampleGroup"
               toolbarPath="sampleGroup"
               id="swtjfacedojo.actions.SampleAction">
         </action>
         <action
               label="Are you sure?"
               icon="icons/sample.gif"
               class="swtjfacedojo.actions.QuestionSampleAction"
               tooltip="Hello, Eclipse world?"
               menubarPath="sampleMenu/sampleGroup"
               toolbarPath="sampleGroup"
               id="swtjfacedojo.actions.QuestionAction">
         </action>
          <action
               label="Custom dialog"
               icon="icons/sample.gif"
               class="swtjfacedojo.actions.CustomDialogAction"
               tooltip="Bye, Eclipse world"
               menubarPath="sampleMenu/sampleGroup"
               toolbarPath="sampleGroup"
               id="swtjfacedojo.actions.CustomDialogAction">
         </action>
      </actionSet>
   </extension>

</plugin>
